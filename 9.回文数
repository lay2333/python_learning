class Solution:
    def isPalindrome(self, x: int) -> bool:
        x=str(x)         #数字转化成字符串                       
        y=[]             #创建一个空列表
        len_x=len(x)     #测量字符串长度
#将x中内容倒装回y中
        for i in range(len_x):
             y.append(x[len_x-1-i])
        y="".join(y)
#比对两者是否相同           
        if x==y:
            return True 
        else:
            return False

#简洁版本
class Solution:
    def isPalindrome(self, x: int) -> bool:
        s = str(x)
        return s == s[::-1] 
#数学方法
class Solution:
    def isPalindrome(self, x: int) -> bool:
            rev = 0
            if x<0 or x>0 and x%10==0:
                return False
            while rev< x//10:
                rev = rev * 10 + x % 10
                x //=10
            return rev == x or rev == x//10
